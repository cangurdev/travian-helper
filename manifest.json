{
    "manifest_version": 3,
    "name": "Travian Plus Ultra",
    "version": "1.0",
    "description": "Travian Plus Ultra",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://*.travian.com/*"],
        "js": ["content.js"]
      }
    ]
  }  